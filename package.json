{
    "license": "MIT",
    "name": "vibrance",
    "description": "Cli colors for deno, built on chalk",
    "version": "0.1.9",
    "main": "dist/node.min.js",
    "module": "dist/module.min.js",
    "source": "main/main.js",
    "exports": {
        "require": "./dist/node.min.js",
        "import": "./dist/module.min.js"
    },
    "scripts": {
        "build": "npx parcel build --target node --target module --no-optimize",
        "pub": "npm version patch && npx parcel build --target node --target module && npx uglifyjs-folder ./dist --each --output ./dist/ && npm publish && git add -A && git commit -m 'build and version bump' && git push"
    },
    "targets": {
        "node": {
            "includeNodeModules": true,
            "distDir": "./dist",
            "source": "main/node.js",
            "engines": {
                "node": "14"
            }
        },
        "module": {
            "includeNodeModules": true,
            "distDir": "./dist",
            "source": "main/module.js"
        }
    },
    "keywords": [
        "color",
        "colour",
        "colors",
        "terminal",
        "console",
        "cli",
        "string",
        "str",
        "ansi",
        "style",
        "styles",
        "tty",
        "formatting",
        "rgb",
        "256",
        "shell",
        "xterm",
        "log",
        "logging",
        "command-line",
        "text",
        "vibrancy",
        "vibrance"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jeff-hykin/vibrance.git"
    },
    "author": "",
    "bugs": {
        "url": "https://github.com/jeff-hykin/vibrance/issues"
    },
    "devDependencies": {
        "chalk": "4.1.2",
        "parcel": "2.0.1",
        "uglifyjs-folder": "3.0.0"
    },
    "homepage": "https://github.com/jeff-hykin/vibrance#readme",
    "dependencies": {
    }
}
